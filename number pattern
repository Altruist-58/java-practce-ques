import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		
  int n ;
       Scanner sc = new Scanner(System.in);
       n = sc.nextInt();
       
    for(int i= 1;i<=n;i++){
		    for(int j=1;j<=n;j++){
		        if(i+j > 5) System.out.print((n+1)-j);
		        else System.out.print(" ");
		    }
		    System.out.println("");
    }  
    }  
    }  
