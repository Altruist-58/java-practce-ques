package com.company;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        // write your code here


        {
            Scanner s = new Scanner(System.in);

            System.out.println("Enter a binary number:");

            String n = s.nextLine();

            System.out.println(Integer.parseInt(n,2));
        }
    }


}
